# Docker User/Group IDs
PUID=1000
PGID=1000

# Timezone
TZ=Europe/Zurich

# Database Configuration
MYSQL_ROOT_PASSWORD=your_secure_root_password
MYSQL_DATABASE=nextcloud
MYSQL_USER=nextcloud
MYSQL_PASSWORD=your_secure_db_password

# Redis Configuration
REDIS_PASSWORD=your_redis_password

# Nextcloud Configuration
NEXTCLOUD_ADMIN_USER=admin
NEXTCLOUD_ADMIN_PASSWORD=your_admin_password

# Domain Configuration
DOMAIN=your-domain.com

# Immich Configuration
IMMICH_API_KEY=your_immich_api_key
IMMICH_UPLOAD_LOCATION=/path/to/your/photos

# Paperless Configuration
PAPERLESS_SECRET_KEY=your_paperless_secret_key
PAPERLESS_OCR_LANGUAGE=deu+eng

# VPN Configuration (if using Gluetun)
VPN_SERVICE=your_vpn_provider
VPN_USER=your_vpn_username
VPN_PASSWORD=your_vpn_password

# SMTP Configuration (for notifications)
SMTP_HOST=your_smtp_server
SMTP_PORT=587
SMTP_USER=your_smtp_username
SMTP_PASSWORD=your_smtp_password
SMTP_FROM_EMAIL=your@email.com

# Backup Configuration
BACKUP_ENCRYPTION_KEY=your_backup_encryption_key

# Monitoring Configuration
PROMETHEUS_PASSWORD=your_prometheus_password
GRAFANA_ADMIN_PASSWORD=your_grafana_password

# API Keys (for external services)
OPENWEATHER_API_KEY=your_openweather_api_key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Security
JWT_SECRET=your_jwt_secret_key
ENCRYPTION_KEY=your_encryption_key

# Application Specific
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=your_n8n_password
PORTAINER_ADMIN_PASSWORD=your_portainer_password